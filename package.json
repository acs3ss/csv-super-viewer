{
  "name": "csv-super-viewer",
  "version": "0.0.1",
  "description": "Read CSV files from the web",
  "main": "index.js",
  "scripts": {
    "webpack": "webpack --mode development --color --progress",
    "webpack:watch": "webpack serve --mode development --color --progress",
    "compile": "tsc -p ./",
    "lint": "eslint src",
    "css": "sass --color --load-path node_modules --style compressed scss/index.scss dist/index.css",
    "css:watch": "sass --watch --color --load-path node_modules --style compressed scss/index.scss dist/index.css",
    "build": "npm run css && webpack --mode production",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "csv"
  ],
  "author": "Austin Sullivan",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^4.6.0",
    "papaparse": "^5.3.0"
  },
  "devDependencies": {
    "@types/bootstrap": "^5.0.7",
    "@types/papaparse": "^5.2.5",
    "@types/wicg-file-system-access": "^2020.9.1",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "copy-webpack-plugin": "^7.0.0",
    "eslint": "^7.20.0",
    "sass": "^1.32.8",
    "ts-loader": "^8.0.17",
    "typescript": "^4.2.2",
    "webpack": "^5.24.2",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  }
}
